<p id="notice"><%= notice %></p>

<h1>Listing Prodapis</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prodapis.each do |prodapi| %>
      <tr>
        <td><%= prodapi.name %></td>
        <td><%= prodapi.price %></td>
        <td><%= prodapi.category_id %></td>
        <td><%= link_to 'Show', prodapi %></td>
        <td><%= link_to 'Edit', edit_prodapi_path(prodapi) %></td>
        <td><%= link_to 'Destroy', prodapi, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Prodapi', new_prodapi_path %>
